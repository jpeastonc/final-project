<h1>
  User type list
</h1>

<a href="/new_user_type_form">
  Add a new user type
</a>

<hr>

<table>
  <tr>
    <th>
      ID
    </th>

    <th>
      Name
    </th>

    <th>
      Created at
    </th>

    <th>
      Updated at
    </th>

    <th>
    </th>
  </tr>

  <% @user_types.each do |user_type| %>
  <tr>
    <td>
      <%= user_type.id %>
    </td>

    <td>
      <%= user_type.name %>
    </td>

    <td>
      <%= time_ago_in_words(user_type.created_at) %> ago
    </td>

    <td>
      <%= time_ago_in_words(user_type.updated_at) %> ago
    </td>

    <td>
      <a href="/user_types/<%= user_type.id %>">
        Show details
      </a>
    </td>
  </tr>
  <% end %>
</table>
